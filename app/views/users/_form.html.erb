
<%= form_with model: @user, local: true do |form| %>
  <p>
  <p>
    <%= form.label :firstname %><br>
    <%= form.text_field :firstname %>
  </p>
  <p>
    <%= form.label :lastname %><br>
    <%= form.text_field :lastname %>
  </p>
   <p>
    <%= form.label :mail %><br>
    <%= form.text_field :mail %>
  </p>
  <p>
    <%= form.label :phone %><br>
    <%= form.text_field :phone %>
  </p>
  <p>
    <%= form.submit %>
  </p>
 
<% end %>